{
    "author": "L3au",
    "title": "gitbook-introduction",
    "plugins": [
        "prism",
        "-highlight",
        "anchors",
        "disqus",
        "editlink",
        "git-author",
        "expandable-chapters"
    ],
    "styles": {
        "website": "styles/website.css"
    },
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "disqus": {
            "shortName": "gitbook-introduction"
        },
        "editlink": {
            "label": "Edit This Page",
            "base": "https://github.com/L3au/gitbook/edit/master/"
        },
        "git-author": {
            "modifyTpl": "最后修改: {user} {timeStamp}",
            "createTpl": "创建: {user} {timeStamp}",
            "timeStampFormat": "YYYY/MM/DD HH:mm"
        }
    }
}
